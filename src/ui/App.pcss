@import '../variables.pcss';

.App {
  display: flex;
  flex-direction: column;
  align-items: center;

  @media (min-width: 501px) {
    width: 600px;
  }

  .header {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    padding: 1rem;
    display: flex;
    flex-direction: column;
    .spacex-logo{
      margin-left: 25px;
      margin-bottom: 10px;
    }
  }

  .main-container {
    width: 100%;
  }
}
